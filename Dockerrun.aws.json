{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "fibweb",
            "image": "michalpowaga/multidockerv2-fibweb",
            "hostname": "fibweb",
            "essential": true,
            "memory": 128
        },
        {
            "name": "proxy",
            "image": "michalpowaga/multidockerv2-proxy",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["fibweb"]
        }
    ]
}